<form id="form-{{ DOMid }}">
   <table id="{{ DOMid }}">
    <caption>{{ title }}</caption>
     {{ #data-list }}
     <tr>
       <td>   
         <label>   
           <input name="{{ groupName }}" id={{ id }} type={{ inputType }}>   
           <div class='control_background'></div>   
           {{ content }}
         </label>   
       </td>   
     </tr> 
     {{ /data-list }}
  </table>
</form>